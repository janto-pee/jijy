mutation CreateAddress {
    createAddress(
        createAddressInput: {
            city: "city"
            country: "country"
            country_code: "country"
            location: "location"
            postal_code: "123"
            state_province_code: "123"
            state_province_name: "12"
            street: "street"
            street2: "street2"
        }
    ) {
        city
        country
        country_code
        location
        postal_code
        state_province_code
        state_province_name
        street
        street2
    }
    createAttribute(
        createAttributeInput: {
            description: "descritpion"
            mandatory: "mandatory"
            name: "name"
            translatable: "tranlatable"
            type: "type"
            value: "value"
            variation: "variation"
        }
    ) {
        description
        mandatory
        name
        translatable
        type
        value
        variation
    }
    createBrand(createBrandInput: { code: 4, name: "string" }) {
        code
        name
    }
    createCategory(
        createCategoryInput: { code: 2.0, description: "string", name: "string" }
    ) {
        code
        description
        name
    }
    createImage(createImageInput: { primary: true, url: "string" }) {
        primary
        url
    }
    createOrder(
        createOrderInput: { businessClientCode: "string", comment: "string", shippingDate: "string" }
    ) {
        businessClientCode
        comment
        shippingDate
    }
    createProduct(
        createProductInput: {
            barcode: "barcode"
            description: "description"
            name: "name"
            parentSku: "parentsku"
            price: "price"
            sellerSku: "sellersku"
            shop: "shop"
            stock: "stock"
        }
    ) {
        description
        name
        parentSku
        price
        sellerSku
        stock
    }
    createShop(createShopInput: { email: "string", name: "string" }) {
        email
        name
    }
    createTag(createTagInput: { icon: "string", name: "string" }) {
        icon
        name
    }
    createVariant(createVariantInput: { icon: "icon", name: "name" }) {
        icon
        name
    }
}


query Addresses {
    Addresses {
        city
        country
        country_code
        location
        postal_code
        state_province_code
        state_province_name
        street
        street2
    }
    Attributes {
        description
        mandatory
        name
        translatable
        type
        value
        variation
    }
    Brands {
        code
        name
    }
    Categorys {
        code
        description
        name
    }
    Images {
        primary
        url
    }
    Orders {
        businessClientCode
        comment
        shippingDate
    }
    Products {
        barcode
        description
        name
        parentSku
        price
        sellerSku
        stock
    }
    Shops {
        email
        name
    }
}